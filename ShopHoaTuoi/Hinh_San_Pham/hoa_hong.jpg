<script id="item-template" type="text/x-handlebars-template">
    {{#each this}}
    <tr class="data-item" data-id='{{Id}}' data-price='{{Price}}'>
        <td>
            <a href='{{ItemUrl}}' title='{{getTooltip ImageUrl}}' class="img-tt">
                <img src='{{getImage ImageUrl}}' alt='{{Name}}' title='{{Name}}' width="52" height="62" />
            </a>
        </td>
        <td>
            <a href='{{ItemUrl}}' title='{{Name}}'>{{Name}}</a>
            <p><strong>Xuất xứ: </strong>{{Manufacturer}}</p>
        </td>
        <td>
            <span class="price {{#checkVip 1}}{{/checkVip}}">Giá sỉ: {{#convertPrice WholesalePrice}}{{/convertPrice}}&nbsp;/&nbsp;{{Unit}}</span>
            <span class="price {{#checkVip 0}}{{/checkVip}}">Giá lẻ: {{#convertPrice RetailPrice}}{{/convertPrice}}&nbsp;/&nbsp;{{Unit}}</span>
        </td>
        <td class="qty">
            <div class="input-group input-group-sm mb-3">
                <div class="input-group-prepend" onclick="addOrRemoveItems(this, false);"><span class="input-group-text"><i class="fas fa-minus"></i></span></div>
                <input type="number" class="txtQty form-control" aria-label="Sô lượng" value="{{getQty Id}}"
                    onchange="handleKeypress(this, event)" onkeyup="handleKeypress(this, event)" />
                <div class="input-group-append" onclick="addOrRemoveItems(this, true);"><span class="input-group-text"><i class="fas fa-plus"></i></span></div>
            </div>
        </td>
        <td class="total">{{getTotal Id}}</td>
    </tr>
    {{/each}}
</script>
<script id="grid-template" type="text/x-handlebars-template">
    {{#each this}}
    <div class="col-6 col-md-3 item data-item" data-id="{{Id}}" data-price='{{Price}}'>
        <div class="row text-center">
            <ul class="icon">
                <li><a href="javascript:void(0);" title="Add to wish list" onclick="addWishList(this, {{Id}});"><i class="{{#getWishList Id}}{{/getWishList}}"></i></a></li>
            </ul>
            <a href='{{ItemUrl}}' title='{{Name}}'>
                <img src='{{getThumbImage ImageUrl}}' alt='{{Name}}' title='{{Name}}' width="150" height="180" />
            </a>
        </div>
        <div class="row">
            <h4><a href='{{ItemUrl}}' title='{{Name}}'>{{Name}}</a></h4>
        </div>
        <div class="row">
            <span class="price {{#checkVip 1}}{{/checkVip}}"><i class="fas fa-home"></i>&nbsp;Giá sỉ:&nbsp;{{#convertPrice WholesalePrice}}{{/convertPrice}}&nbsp;/&nbsp;{{Unit}}</span>
            <span class="price {{#checkVip 0}}{{/checkVip}}"><i class="fas fa-user"></i>&nbsp;Giá lẻ:&nbsp;{{#convertPrice RetailPrice}}{{/convertPrice}}&nbsp;/&nbsp;{{Unit}}</span>
        </div>
        <div class="row">
            <div class="input-group input-group-sm mb-3">
                <div class="input-group-prepend" onclick="addOrRemoveItems(this, false);"><span class="input-group-text"><i class="fas fa-minus"></i></span></div>
                <input type="number" class="txtQty form-control" aria-label="Sô lượng" value="{{getQty Id}}"
                    onchange="handleKeypress(this, event)" onkeyup="handleKeypress(this, event)" placeholder="Số lượng" />
                <div class="input-group-append" onclick="addOrRemoveItems(this, true);"><span class="input-group-text"><i class="fas fa-plus"></i></span></div>
            </div>
        </div>
    </div>
    {{/each}}
</script>
